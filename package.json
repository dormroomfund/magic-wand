{
  "name": "magic-wand",
  "description": "pipeline automation for Dorm Room Fund",
  "version": "0.0.0",
  "private": true,
  "main": "server",
  "author": {
    "name": "Dorm Room Fund",
    "email": "team@drf.vc"
  },
  "directories": {
    "lib": "server",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^11.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && NODE_ENV= npm run jest",
    "lint": "eslint server/. client/. test/. --ext js,jsx --config .eslintrc.json",
    "lint-fix": "eslint --fix server/. client/. test/. --ext js,jsx --config .eslintrc.json",
    "prettier": "prettier --write \"{server,client,test}/**/*.js\" \"config/**/*.json\"",
    "dev": "nodemon --exec babel-node server/",
    "build": "next build client",
    "start": "babel-node server/",
    "jest": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.15",
    "@feathersjs/authentication-jwt": "^2.0.9",
    "@feathersjs/authentication-oauth2": "^1.2.7",
    "@feathersjs/configuration": "^2.0.6",
    "@feathersjs/errors": "^3.3.6",
    "@feathersjs/express": "^1.3.1",
    "@feathersjs/feathers": "^3.3.1",
    "@feathersjs/socketio": "^3.2.9",
    "colors": "^1.3.3",
    "compression": "^1.7.3",
    "config": "^3.0.1",
    "cors": "^2.8.5",
    "feathers-objection": "^2.0.0",
    "feathers-sequelize": "^4.0.7",
    "helmet": "^3.15.0",
    "knex": "^0.16.3",
    "next": "^7.0.2",
    "next-routes": "^1.4.2",
    "objection": "^1.4.0",
    "passport-auth0": "^1.1.0",
    "pg": "^7.7.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "sequelize": "^4.42.0",
    "serve-favicon": "^2.5.0",
    "tcomb": "^3.2.29",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-eslint-comments": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.2",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.9",
    "prettier": "1.15.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  }
}
