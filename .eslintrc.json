{
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "plugins": ["react", "import"],
  "extends": [
    "airbnb",
    "plugin:prettier/recommended",
    "plugin:eslint-comments/recommended"
  ],
  "rules": {
    "import/no-unresolved": "off",
    "import/prefer-default-export": "off",
    "func-names": "off",
    "prettier/prettier": "error",
    "require-jsdoc": "off",
    "valid-jsdoc": "off",
    "no-param-reassign": "off"
  }
}
